{
	"name": "charm-dev",
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"DevPackages": {
				"$path": "DevPackages",
				"Charm": {
					"$path": "packages/charm"
				},
				"CharmSync": {
					"$path": "packages/charm-sync"
				},
				"ReactCharm": {
					"$path": "packages/react"
				}
			}
		},
		"TestService": {
			"$className": "TestService",
			"$properties": {
				"ExecuteWithStudioRun": true
			},
			"Tests": {
				"$path": "tests"
			}
		}
	}
}
